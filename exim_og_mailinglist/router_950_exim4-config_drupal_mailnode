# this is a custom router for the Drupal module og_mailinglist

drupal_mailnode:
  driver = queryprogram
  command = /etc/exim4/exim_og_mailinglist/og_mailinglist_exim4_router.php $local_part $domain $sender_address_local_part $sender_address_domain $return_path
  command_user = mail
  command_group = mail
  transport = drupal_mailnode 
